{
  "name": "ksac",
  "version": "1.2.0",
  "description": "Knowledge Source as Code for StackSpot AI",
  "main": "dist/index.js",
  "bin": {
    "ksac": "bin/ksac.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "test": "jest",
    "test:e2e": "jest ./src/e2e",
    "prepare": "npm run build",
    "lint": "eslint \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabriel-pinheiro/ksac.git"
  },
  "keywords": [
    "StackSpot"
  ],
  "author": "Gabriel Pinheiro <gabrielctpinheiro@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/gabriel-pinheiro/ksac/issues"
  },
  "homepage": "https://github.com/gabriel-pinheiro/ksac#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@hapi/hoek": "^11.0.4",
    "@mocko/sync": "^0.2.1",
    "colors": "^1.4.0",
    "commander": "^12.1.0",
    "debug": "^4.3.5",
    "gp-simple-update-notifier": "^2.1.0",
    "hcl-parser": "^0.1.1",
    "inversify": "^6.0.2",
    "joi": "^17.13.3",
    "prompts": "^2.4.2",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.6.2",
    "stkai-sdk": "^1.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
